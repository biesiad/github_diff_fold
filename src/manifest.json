{
  "manifest_version": 2,
  "name": "Github diff fold",
  "description": "Collapse/expand button for code blobs on Github.com",
  "version": "1.0.0",
  "options_page": "options.html",
  "permissions": [ "storage" ],
  "content_scripts": [{
    "matches": [
      "*://github.com/*/pull*",
      "*://github.com/*/commit/*"
    ],
    "js": [ "content_scripts.js" ]
  }],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
